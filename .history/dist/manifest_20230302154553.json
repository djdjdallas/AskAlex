{
  "name": "Ask Alex ",
  "version": "1.0",
  "description": "Generate suggested prompts/questions using the OpenAI API",
  "manifest_version": 3,

  "action": {
    "default_popup": "popup.html",
    "default_title": "Ask Alex"
  },
  "icons": {
    "16": "/1t6.png",
    "32": "/4t8.png",
    "48": "/4t8.png",
    "128": "/128t.png"
  },
  "permissions":[
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
  
  
}
